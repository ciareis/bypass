{
    "name": "ciareis/bypass",
    "description": "Bypass provides a quick way to create a custom plug that can be put in place instead of an actual HTTP server to return prebaked responses to client requests.",
    "keywords": [
        "bypass",
        "php",
        "http",
        "faker",
        "api"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Leandro Henrique",
            "email": "emtudo@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "illuminate/support": "^5.5 || ^6.0 || ^7.0 || ^8.0",
        "symfony/process": "^4.4.21 || ^5.2.4"
    },
    "require-dev": {
        "laravel/framework": "^8.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^8.5|^9.0",
        "guzzlehttp/guzzle": "^7.3",
        "pestphp/pest": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Ciareis\\Bypass\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\" : "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ciareis\\Bypass\\BypassServiceProvider"
            ]
        }
    }
}
